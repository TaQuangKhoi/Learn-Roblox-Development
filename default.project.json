{
  "name": "Learn-Roblox-Development",
  "tree": {
    "$className": "DataModel",

    "Players":{
      "$className": "Players",
      "$properties": {
        "RespawnTime": 3
      }
    },

    "ReplicatedStorage": {
      "Common": {
        "$path": "src/shared"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {

          "Anchored": true,
          "Transparency": 0,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],

          "Locked": true,

          "Position": [

            0,
            -10,
            0
          ],
          "Size": [
            512,
            30,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}